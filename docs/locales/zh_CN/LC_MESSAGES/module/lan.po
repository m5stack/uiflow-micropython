msgstr "LAN 状态码，可能的值："

#: ../../source/module/lan.rst:111 58f64f38246b4113bdeb3bb71e208318
msgid "network.ETH_INITIALIZED(0): Ethernet interface initialized"
msgstr "network.ETH_INITIALIZED(0)：以太网接口已初始化"

#: ../../source/module/lan.rst:112 d416e1ad8bfa4595b0cbf9bff9a514d8
msgid "network.ETH_STARTED(1): Ethernet driver started"
msgstr "network.ETH_STARTED(1)：以太网驱动已启动"

#: ../../source/module/lan.rst:113 7e18b2b6d1bc4e75a22f5ed7c5f80d80
msgid "network.ETH_STOPPED(2): Ethernet driver stopped"
msgstr "network.ETH_STOPPED(2)：以太网驱动已停止"

#: ../../source/module/lan.rst:114 c31bdf2fde4044febd2a71da4fd5f6f0
msgid "network.ETH_CONNECTED(3): Physical link established (cable connected)"
msgstr "network.ETH_CONNECTED(3)：物理链路已建立（网线已连接）"

#: ../../source/module/lan.rst:115 dccbbb63bf5d4bcfabaf399fc4ca86ff
msgid "network.ETH_DISCONNECTED(4): Physical link lost (cable disconnected)"
msgstr "network.ETH_DISCONNECTED(4)：物理链路丢失（网线已断开）"

#: ../../source/module/lan.rst:116 017dc384cbc846e49ee357feb032a67e
